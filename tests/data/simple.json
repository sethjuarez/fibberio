{
  "sources": [
    {
      "id": "names",
      "data": "./full_names.csv"
    }
  ],
  "features": [
    {
      "feature": "FirstName,LastName",
      "source": "names",
      "distribution": "uniform"
    },
    {
      "feature": "Age",
      "source": "(14, 85] -> int",
      "distribution": "normal(mean=1, stddev=.2)"
    },
    {
      "feature": "TabsVSpaces",
      "source": ["tabs", "spaces"],
      "distribution": [23, 2344]
    }
  ]
}

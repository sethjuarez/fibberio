{
    "sources": {
        "names": {
            "path": "./full_names.csv",
            "read_csv": {
                "encoding": "unicode_escape",
                "engine": "python"
            }
        }
    },
    "features": {
        "name": {
            "source": {
                "id": "names",
                "target": "FirstName, LastName"
            }
        },
        "age": {
            "normal": {
                "mean": 25,
                "stddev": 1.2
            }
        },
        "farther": {
            "uniform": {
                "low": 14,
                "high": 85,
                "itype": "float",
                "precision": 2
            },
            "conditional": {
                "score": {
                    "[14, 65)": {
                        "uniform": {
                            "low": 5,
                            "high": 25,
                            "itype": "float",
                            "precision": 2
                        }
                    },
                    "[65, *)": {
                        "normal": {
                            "mean": 35,
                            "stddev": 0.5
                        }
                    }
                }
            }
        },
        "TabsVSpaces": {
            "discrete": {
                "tabs": 234,
                "spaces": 2332
            },
            "conditional": {
                "score": {
                    "tabs": {
                        "uniform": {
                            "low": 5,
                            "high": 25,
                            "itype": "float",
                            "precision": 2
                        }
                    },
                    "spaces": {
                        "normal": {
                            "mean": 35,
                            "stddev": 0.5
                        }
                    }
                },
                "conditional": {
                    "accept": {
                        "[14, 25)": {
                            "discrete": {
                                "True": 23,
                                "False": 2123
                            }
                        },
                        "[25, *]": {
                            "discrete": {
                                "True": 23,
                                "False": 2123
                            }
                        },
                        "*": {
                            "discrete": {
                                "True": 2232,
                                "False": 10
                            }
                        }
                    }
                }
            }
        }
    }
}